            .replace(/^\.?\/item/, "")
            .replace(/^\.?\/text/, "")
            .replace(/^\.?\/label/, "")
            .replace(/^\.?\//, "");
          newObj[key] = propertyValue as ResolvedValue;
          continue;
        }

        newObj[key] = this.#resolvePropertyValue(
          propertyValue,
          surface,
          visited,
          dataContextPath,
          idSuffix,
          key
        );
      }
      return newObj;
    }

    // 5. Otherwise, it's a primitive value.
    return value as ResolvedValue;
  }
}
